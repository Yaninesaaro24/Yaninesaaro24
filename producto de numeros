#include <iostream>

using namespace std;

int main() {
    int N;
    int producto = 1;

    cout << "Ingrese un número entero positivo N: ";
    cin >> N;
    if (N <= 0) {
        cout << "N debe ser un número entero positivo." << endl;
    } else {
        for (int i = 1; i <= N; i++) {
            producto *= i;
        }
        cout << "El producto de los primeros " << N << " numeros enteros es: " << producto << endl;
    }

    return 0;
}

